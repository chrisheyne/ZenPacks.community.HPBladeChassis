<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane">

<tal:block 
    tal:define="tableName string:bladeserversList;
    objects here/bladeservers/objectValuesAll;
    tabletitle string:Blade Server Information;
    batch python:here.ZenTableManager.getBatch(tableName,objects)">
<tal:block metal:use-macro="here/zenuimacros/macros/zentable">
<tal:block metal:fill-slot="zentablecontents">

<!-- BEGIN TABLE CONTENTS -->
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsDisplayName','Name')"/>
	<th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsPosition', 'Position')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsProductId','Product Id')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsSerialNum','Serial #')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsSlotsUsed','Slots Used')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsHeight','Height')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsWidth','Width')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(tableName,'bsDepth','Depth')"/>
    </tr>
    <tal:block tal:repeat="gd batch">
    <tr tal:define="odd repeat/gd/odd; link gd/managedDeviceLink"
        tal:attributes="class python:test(odd, 'odd', 'even')">
        <td class="tablevalues">
        <span tal:content="structure gd/urlLink"/>
        <span tal:condition="link" tal:omit-tag>
                 (<span tal:replace="structure gd/managedDeviceLink"/>)
            </span>
        </td>
	<td class="tablevalues" tal:content="gd/bsPosition"/>
        <td class="tablevalues" tal:content="gd/bsProductId"/>
        <td class="tablevalues" tal:content="gd/bsSerialNum"/>
        <td class="tablevalues" tal:content="gd/bsSlotsUsed"/>
        <td class="tablevalues" tal:content="gd/bsHeight"/>
        <td class="tablevalues" tal:content="gd/bsWidth"/>
        <td class="tablevalues" tal:content="gd/bsDepth"/>
    </tr>
    </tal:block>
    <tr>
        <td colspan="8" class="tableheader" align='center'>
        <form metal:use-macro="here/zenTableNavigation/macros/navtool"></form>
        </td>
    </tr>
<!-- END TABLE CONTENTS -->

</tal:block>
</tal:block>
</tal:block>

</tal:block>
</tal:block>


